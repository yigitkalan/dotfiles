{
    "coc.source.around.priority": -10,
    "coc.source.buffer.priorty": -10,
    "coc.source.around.enable": false,
    "coc.source.buffer.enable": false,
    "diagnostic.errorSign": "❌",
    "diagnostic.warningSign": "👉",
    "workspace.ignoredFolders": [
        "$HOME",
        "$HOME/.cargo/**",
        "$HOME/.rustup/**"
    ],
    "outline.autoPreview": true,
    "diagnostic.checkCurrentLine": true,
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-17",
            "path": "/usr/lib/jvm/java-17-openjdk",
            "default": true
        }
    ],
    "suggest.completionItemKindLabels": {
        "method": "  Method",
        "function": "  Function",
        "variable": "[] Variable",
        "field": "  Field",
        "typeParameter": "<> TypeParam",
        "constant": "  Constant",
        "class": "  Class",
        "interface": "  Interface",
        "struct": "  Struct",
        "event": "  Event",
        "operator": "  Operator",
        "module": " 󰅩 Module",
        "property": "  Property",
        "enum": "  Enum",
        "reference": "  Reference",
        "keyword": "  Keyword",
        "file": "  File",
        "folder": " 󰉋 Folder",
        "color": "  Color",
        "unit": "  Unit",
        "snippet": "  Snippet",
        "text": "  Text",
        "constructor": "  Constructor",
        "value": "  Value",
        "enumMember": "  EnumMember"
    },
    "coc.preferences.formatOnSave": false,
    "Lua.diagnostics.globals": [
        "vim"
    ],
    "suggest.floatConfig": {
        "border": true,
        "rounded": true

    },
    "diagnostic.floatConfig": {
        "border": true,
        "rounded": true
    },
    "signature.floatConfig": {
        "border": true,
        "rounded": true
    },
    "hover.floatConfig": {
        "border": true,
        "rounded": true
    },
    "html-css-support.styleSheets": [
        "/style.css",
        "style.css",
        "node_modules/bootstrap/dist/css/bootstrap.css",
        "https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css",
        "src/app/globals.css"
    ],
    "html-css-support.enabledLanguages": [
        "html",
        "vue",
        "blade",
        "htmldjango",
        "typescriptreact",
        "javascriptreact"
    ],
    "languageserver": {
        "csharp-ls": {
            "command": "csharp-ls",
            "filetypes": [
                "cs"
            ],
            "rootPatterns": [
                "*.sln",
                "*.csproj",
                ".vim/",
                ".git/",
                ".hg/"
            ]
        }
    },

    "rust-analyzer.check.command": "clippy"
}
